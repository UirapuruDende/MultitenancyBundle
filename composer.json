{
  "name": "dende/multitenancybundle",
  "description": "Multitenancy Bundle",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Grzegorz 'Uirapuru' Kaszuba",
      "email": "uirapuruadg@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "doctrine/dbal": "^2",
    "symfony/symfony": "^2|^3",
    "sensio/framework-extra-bundle": "^3",
    "doctrine/doctrine-bundle": "^1",
    "doctrine/orm": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5|^6",
    "symfony/monolog-bundle": "^3",
    "liip/functional-test-bundle": "^1",
    "twig/twig" : "^1",
    "sebastian/global-state" : "^1",
    "sebastian/comparator" : "^1"
  },
  "autoload": {
    "psr-4": {
      "Dende\\MultitenancyBundle\\": "src",
      "Tests\\App\\" : "tests/app/src",
      "Tests\\Unit\\" : "tests/unit",
      "Tests\\Functional\\" : "tests/functional"
    }
  },
  "autoload-dev": {
    "files": ["tests/app/AppKernel.php"]
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "source",
    "platform": {
      "php": "7.1"
    }
  }
}
