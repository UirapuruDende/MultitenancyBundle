{
  "name": "dende/multitenancybundle",
  "description": "Multitenancy Bundle",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Grzegorz 'Uirapuru' Kaszuba",
      "email": "uirapuruadg@gmail.com"
    }
  ],
  "require": {
    "doctrine/dbal": "v2.5.10",
    "symfony/symfony": "v2.8.16",
    "sensio/framework-extra-bundle": "v3.0.19",
    "doctrine/doctrine-bundle": "1.6.7",
    "doctrine/doctrine-fixtures-bundle": "2.3.0",
    "doctrine/orm": "v2.5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.9",
    "symfony/monolog-bundle": "3.0.1",
    "liip/functional-test-bundle": "1.7.0",
    "twig/twig" : "^1",
    "sebastian/global-state" : "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Dende\\MultitenancyBundle\\": "src",
      "Tests\\App\\" : "tests/app/src",
      "Tests\\Unit\\" : "tests/unit",
      "Tests\\Functional\\" : "tests/functional"
    }
  },
  "autoload-dev": {
    "files": ["tests/app/AppKernel.php"]
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": "source"
  }
}
